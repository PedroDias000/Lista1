#include <stdio.h>

int main() {
    int x, n;

    printf("Digite o valor de x: ");
    scanf("%d", &x);

    printf("Digite o valor de n: ");
    scanf("%d", &n);

    int resultado = x << n;  // Calcula x * 2^n usando um deslocamento à esquerda (shift left).

    printf("O resultado é: %d\n", resultado);

    return 0;
}
