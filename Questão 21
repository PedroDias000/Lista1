#include <stdio.h>

int main() {
    int num1, num2;

    printf("Digite dois números inteiros: ");
    scanf("%d %d", &num1, &num2);

    int Multiple = (num1 % num2) == 0;

    printf("%s\n", Multiple ? "O primeiro número é múltiplo do segundo." : "O primeiro número não é múltiplo do segundo.");

    return 0;
}
